#include <stdio.h>
#include <stdlib.h>

struct student {
	int number;
	char name[10];
	double grade;
};

int main(void)
{
		
	struct student s;
	s.number = 200301759;
	strcpy(s.name, "Shin ho jin");
	s.grade = 3.55;
	printf("학번 : %d\n", s.number);
	printf("이름 : %s\n", s.name);
	printf("학점 : %.1\n", s.grade);
	return 0;
}
